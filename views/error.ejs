<% layout('layouts/boilerplate')%>
    <div class="row">
        <div class="">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">Error <%=err.statusCode%>: <%=err.message%>
                </h4>
                <hr>
                <% if (currentUser && currentUser.isSuper) { %>
                    <p>
                        <%= err.stack %>
                    </p>
                    <% } %>
            </div>
        </div>
    </div>